<h1 class="red_section big">
	Les <span>ÉVÈNEMENTS</span>
	<% if @category %>
		 de la catégorie <span>"<%= @category.name %>"</span>
	<% end %>
</h1>
<section id="events" >
	<header>
		<h1>Derniers évènements ajoutées</h1>
	</header>
	<%= render "events/events" %>



	<% if can? :new, @event %><%= link_to 'New Event', new_event_path %><% end %>
</section>

<%= paginate @events %>